#!/usr/bin/env zsh

# This file is sourced from ../runcom/zshenv

# Setup path
path=(
$path
$HOME/.pyenv
/usr/local/sbin
$HOME/.local/bin
$HOME/.cargo/bin
$HOME/.fnm
$HOME/.yarn/bin
$HOME/.config/yarn/global/node_modules/.bin
)

# Ensure path arrays do not contain duplicates
typeset -gU cdpath fpath mailpath path

